<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp3_1.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Spacing="25"
            Padding="16,0"
            VerticalOptions="Center">

            <Entry
                x:Name="PhoneNum">

            </Entry>
            <Button
                x:Name="DialBtn"
                Text="Dial"
                Clicked="OnDial">
            </Button>
            
        </VerticalStackLayout>
    </ScrollView>
    
</ContentPage>
